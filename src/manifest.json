{
  "manifest_version": 2,
  "name": "Practice Forcius",
  "description": "A CodeForces extension to quickly pick up a problem and solve it.",
  "version": "0.1.0",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/vendor.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "https://codeforces.com/*",
    "activeTab",
    "notifications"
  ]
}
