{
  "manifest_version": 2,
  "name": "Practice Forcius",
  "description": "An extension to use your practice forces.",
  "version": "0.1.0",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://codeforces.com/*",
    "activeTab",
    "notifications"
  ]
}
